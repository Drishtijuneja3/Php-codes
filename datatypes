#code to demonstrate various data types in python
#Numbers
integer=42
floating=3.2
complex_num=1+2j
#printing out
print("Integer:" + str(integer))
print("Float:" + str(floating))
print("Complex number:" + str(complex_num))
#Boolean
boolean=True
print("Boolean:", boolean)
#sequence
string="Hello, world!"
tuple_seq=(1,2,3)
list_seq=[1,2,3]
print("String:" , string)
print("Tuple:" , tuple_seq)
print("List:" , list_seq)
#sets
set_type={1,2,3}
print("Set:" , set_type)
#Mapping
dictionary={"key":"value"}
print("Disctionary:" , dictionary)
#None
none_type=none
print("None:" , none_type)
